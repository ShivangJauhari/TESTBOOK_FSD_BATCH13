<h1>Login</h1>

<% if(typeof message !== 'undefined') { %>
    <h4><%= message %></h4>
<% } %>

<form action="" method="post">
    <input type="email" placeholder="Email" name="email" required>
    <br><br>
    <input type="password" name="password" placeholder="password" required>
    <br><br>
    <input type="submit" value="Login">
</form>